.example#example
  .container

    +titleBlock("ПРИМЕРЫ ", "РАБОТ")

    .example__row.wow.wowFadeUp(data-animation-delay="0.85s")
      .swiper-container.js-sliderExample

        .swiper-wrapper
          .swiper-slide
            img(
              src="./img/img-example__forms-0.png"
              srcset="./img/img-example__forms-0@2x.png 2x"
              class="example__img"
              alt=""
              zoom-img-js
            )
          .swiper-slide
            img(
              src="./img/img-example-3.png"
              srcset="./img/img-example-3@2x.png 2x"
              class="example__img"
              alt=""
              zoom-img-js
            )
          .swiper-slide
            img(
              src="./img/img-example-4.png"
              srcset="./img/img-example-4@2x.png 2x"
              class="example__img"
              alt=""
              zoom-img-js
            )

        .swiper-button-next
        .swiper-button-prev
      .swiper-pagination
