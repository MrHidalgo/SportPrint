.instagram
  .container

    +titleBlock("НАШ", "ИНСТАГРАМ")

    .instagram__row
      -for(var i = 0; i < 8; i++)
        .instagram__block.wow.wowFadeUp(data-animation-delay="0." + (i + 2) + "5s")

          img(
            src="./img/img__instagram-" + i + ".jpg"
            srcset="./img/img__instagram-" + i + "@2x.jpg 2x"
            alt=""
          )

